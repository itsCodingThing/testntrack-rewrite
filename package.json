{
  "name": "testntrackapi",
  "private": true,
  "type": "module",
  "scripts": {
    "server:prod": "NODE_ENV=production node ./build/server.js",
    "server:dev": "NODE_ENV=development node --enable-source-maps --watch-path=./build ./build/server.js",
    "lint": "eslint --ext .ts src",
    "clean": "rimraf build/ tsconfig.tsbuildinfo tsconfig.prod.tsbuildinfo",
    "pm2": "pm2 start ecosystem.config.cjs --env production --only tnt_backend",
    "start:prod": "npm run clean && tsc -p tsconfig.json && swc src -d build --copy-files && npm run server:prod",
    "start:dev": "npm run clean && conc -c 'auto' --n swc,tsc,server 'swc src -d build --copy-files -w' 'tsc -p tsconfig.json --watch' 'npm run server:dev'"
  },
  "prisma": {
    "seed": "tsx src/utils/db-seed/seed.ts"
  },
